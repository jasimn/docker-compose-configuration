# Project Configuration
PROJECT_NAME=mongodb-setup
COMPOSE_PROJECT_NAME=mongodb_setup

# MongoDB Configuration
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=SecurePassword123!
MONGO_INITDB_DATABASE=myapp
MONGO_HOST_PORT=27017

# Mongo Express Configuration
ME_USERNAME=admin
ME_PASSWORD=ExpressPass123!
ME_HOST_PORT=8081
ME_READONLY=false
ME_COOKIE_SECRET=mySuperSecretCookieKey123
ME_SESSION_SECRET=mySuperSecretSessionKey123
ME_REQUEST_SIZE=10mb
ME_ENABLE_ADMIN=true

# Optional: Development Flags
NODE_ENV=development
DEBUG=false
